                }
            ]
        },
        {
            "name": "webkitgtk4",
            "cmake": true,
            "build-options" : {
                "cflags": "-O2 -g1",
                "cxxflags": "-O2 -g1",
                "arch" : {
                    "i386" : {
                        "config-opts" : [
                            "-DCMAKE_SYSTEM_PROCESSOR=i586"
